			<condcompareto>DoNotIncludeNUnitSolution</condcompareto>
			<condcomparison type='3'>2</condcomparison>
			<condexpr>%FastBuildSwitches%</condexpr>
			<indent type='3'>4</indent>
			<name>Set DoNotIncludeNUnitSolution</name>
		</step>
