			<indent type='3'>2</indent>
			<name>Default loop counter BUILD_BRAND_INDEX</name>
		</step>
		<step action='Set Macro'>
			<MacroName>BRAND_LOOP_UBOUND_VNEXT</MacroName>
			<MacroValue>[%BRAND_LOOP_COUNT_VNEXT%+1]</MacroValue>
