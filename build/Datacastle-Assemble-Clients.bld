			<indent type='3'>2</indent>
			<name>Default the loop upper boud to one more than the brand selected</name>
		</step>
		<step action='Group'>
			<buildfailsteps type='11'>0</buildfailsteps>
			<condcomparison type='3'>1</condcomparison>
			<condexpr>%BuildAllClientBrands%</condexpr>
			<indent type='3'>2</indent>
			<name>Reset loop start and counter if building all</name>
		</step>
		<step action='Set Macro'>
			<MacroName>BRAND_LOOP_COUNT_VNEXT</MacroName>
			<MacroValue>0</MacroValue>
			<condexpr>%BuildAllClientBrands%</condexpr>
